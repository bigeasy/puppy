#!/bin/bash

dir=$(/bin/readlink -f "$(/usr/bin/dirname $0)/../..")

$dir/bin/instance/launch --name alvar.balance.south.california.runpup.com  &
$dir/bin/instance/launch --name alvar.balance.north.california.runpup.com  &
$dir/bin/instance/launch --name alvar.user.south.california.runpup.com  &
$dir/bin/instance/launch --name alvar.user.north.california.runpup.com  &
$dir/bin/instance/launch --name alvar.data.south.california.runpup.com  &
$dir/bin/instance/launch --name alvar.data.north.california.runpup.com  &
