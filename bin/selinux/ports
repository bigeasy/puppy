#!/bin/bash

# The output file is checked into source control to detect changes to the set of
# default ports. The list is always generated on a machine that hosts only the
# default Fedora SELinux policy, and no Puppy generated policies.

/usr/bin/sudo /usr/sbin/semanage port -l | /bin/sed 1d | puppy/bin/filter_selinux_ports  | /bin/sort -g | /usr/bin/uniq > bin/ports.txt
