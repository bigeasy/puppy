#!/bin/bash

dir=$(/bin/readlink -f "$(/usr/bin/dirname $0)/../../..")

function deploy()
{
    branch=$1
    /puppy/janitor/bin/deploy $dir/$branch
}

$dir/janitor/puppy/janitor/bin/deploy $dir/janitor

deploy system
deploy common
deploy private
deploy protected
deploy public
deploy worker
deploy vhosts
deploy database
deploy liminal
deploy janitor
deploy exclusive
