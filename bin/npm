#!/bin/bash

dirname=$(dirname $0)

function npm_install()
{
    branch=$1
    cd $HOME/git/ecma/puppy/$branch
    /usr/bin/cake compile
    /usr/bin/sudo /usr/bin/npm --globalconfig $HOME/git/ecma/puppy/system/bin/npmrc install .
}

npm_install common
npm_install private
npm_install protected
npm_install public
npm_install enqueue
npm_install worker

/usr/bin/sudo /sbin/restorecon -R /opt
